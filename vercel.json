{
    "version": 2,
    "builds": [
        {
            "src": "WebRetrieve_Autonoma/api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "WebRetrieve_Autonoma/api/main.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "PLAYWRIGHT_BROWSERS_PATH": "/tmp/ms-playwright"
    },
    "functions": {
        "WebRetrieve_Autonoma/api/main.py": {
            "maxDuration": 30
        }
    }
}